{
  "version": 1,
  "paths": {
    "/questions": {
      "name": "/questions",
      "lambdaFunction": "questionsLambda",
      "permissions": {
        "setting": "private",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ]
      }
    },
    "/topics": {
      "name": "/topics",
      "lambdaFunction": "topicLambda",
      "permissions": {
        "setting": "open"
      }
    }
  }
}